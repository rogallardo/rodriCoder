<div class="realtimeproductspage-container">
    <h1 class="title-products">Products</h1>


    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Title</th>
                <th scope="col">Description</th>
                <th scope="col">Price</th>
                <th scope="col">Stock</th>
                <th scope="col">Category</th>
                <th scope="col">Thumbnail</th>
            </tr>
        </thead>
        <tbody id="tbodyproducts">
            {{#each products}}
            <tr class="prueba">
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.thumbnail}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <table id="table-container" class="default">
    </table>
</div>
